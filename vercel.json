{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ],
  "projectSettings": {
    "framework": "node",
    "buildCommand": "npm install",
    "outputDirectory": ".",
    "devCommand": "npm run dev"
  }
}